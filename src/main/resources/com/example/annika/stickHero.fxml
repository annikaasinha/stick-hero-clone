                <Image url="@stickhero-free-full-version-1.jpg" />
            </image>
        </ImageView>
        <Button fx:id="startButton" layoutX="263.0" layoutY="264.0" mnemonicParsing="false" onAction="#handleStartButton" prefHeight="42.0" prefWidth="65.0" text="Start" />
